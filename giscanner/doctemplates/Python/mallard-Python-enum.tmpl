<%inherit file="mallard-base.tmpl"/>
<%block name="content">
  ${formatter.format(node, node.doc)}
% if node.members:
<table>
% for member, ix in zip(node.members, range(len(node.members))):
<tr>
<td><p>${node.name}.${member.name.upper()} :</p></td>
<td>${formatter.format(node, member.doc)}</td>
</tr>
% endfor
</table>
% endif
</%block>
