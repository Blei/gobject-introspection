<?xml version="1.0" encoding="UTF-8"?>

<tests xmlns="http://schemas.gnome.org/gobject-introspection/2013/test">

<test>
  <input>/**
 * annotation_versioned:
 *
 * Stability: Stable
 **/</input>
  <parser>
    <docblock>
      <identifier>
        <name>annotation_versioned</name>
      </identifier>
      <tags>
        <tag>
          <name>stability</name>
          <description>Stable</description>
        </tag>
      </tags>
    </docblock>
  </parser>
</test>

